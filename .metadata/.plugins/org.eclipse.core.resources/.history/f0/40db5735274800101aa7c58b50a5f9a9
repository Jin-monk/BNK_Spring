package com.example._st_test.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MemberController {

	
	
	@GetMapping("/")
	public String root() {
		return "/member/index";
	}
	
	@GetMapping("/join")
	public String join () {
		return "join";
	}
}
